        <pose frame=''>0 0 0 0 0 0</pose>
        <mass>0.005</mass>	<!-- From 0.14kg to 0.005kg -->
        <inertia>
          <ixx>0.08333</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.08333</iyy>
          <iyz>0</iyz>
