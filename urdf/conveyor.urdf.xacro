<!--
Belt Width:15.7inch ; 
Belt Thickness:0.07inch ; 
Conveyor Length:59inch
Conveyor High:29.5inch  
-->  
  <xacro:arg name="gazebo" default="false"/>
  <xacro:arg name="load_rollers" default="false"/>
  <xacro:arg name="load_motors" default="false"/>
  <xacro:arg name="load_railings" default="true"/>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="darkgray">
    <color rgba=".2 .2 .2 1"/>
  </material>
  <material name="darkred">
    <color rgba=".5 .1 .1 1"/>
  </material>
  <material name="ur3e_red">
    <color rgba=".5 .1 .1 1"/>
  </material>
  <material name="ur3e_gray">
    <color rgba="0.75294 0.75294 0.75294 1"/>
  </material>

  <link name="world"/>
  <joint name="world_to_frame" type="fixed">
    <parent link="world"/>
    <child link="frame"/>
   <xacro:if value="$(arg gazebo)">
    <dynamics damping="10.0" friction="5.0"/>
   </xacro:if>
  </joint>

    <link name="frame" type="fixed" >
       <visual>
	<origin rpy="0 0 0" xyz="0.75 -0.75 0" />
  	  <geometry>
		<mesh filename="package://ur3e_irl_project/meshes/frame.stl"/>
	  </geometry>
       </visual>

	<collision>
          <origin rpy="0 0 0" xyz="0.75 -0.75 0" />
  	  <geometry>
		<mesh filename="package://ur3e_irl_project/meshes/conveyor_box.stl"/>
	  </geometry>
	</collision>

    <inertial>
       <origin rpy="0 0 0" xyz="0.01 0.75 0.46"/>
       <mass value="29.18"/>
      <inertia ixx="30.49" ixy="0.13" ixz="0.10" iyy="9.39" iyz="10.09" izz="22.63"/>
      </inertial>
    </link>

  <joint name="frame_to_belt" type="fixed">
    <origin rpy="0 0 0" xyz="0.75 0 0.73" />

    <parent link="frame"/>
    <child link="belt"/>
   <xacro:if value="$(arg gazebo)">
    <dynamics damping="10.0" friction="5.0"/>
   </xacro:if>
   </joint>

   <link name="belt" type="fixed" > 
       <visual>
	<origin rpy="0 0 0" xyz="0 0 0" />
  	  <geometry>
		<mesh filename="package://ur3e_irl_project/meshes/belt.stl"/>
	  </geometry>
       </visual>

	<collision>
          <origin rpy="0 0 0" xyz="0 0 0" />
  	  <geometry>
		<mesh filename="package://ur3e_irl_project/meshes/belt.stl"/>
	  </geometry>
	</collision>

    <inertial>
       <origin rpy="0 0 0" xyz="0 0.7 0"/>
       <mass value="1.75"/>
      <inertia ixx="1.21" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="1.2"/>
    </inertial>
    </link>

<xacro:if value="$(arg load_railings)">

<!-- The reason why these are initialized w.r.t the world frame is because
we want floating joints above the conveyor and w.r.t the frame will not do that -->


  <joint name="frame_railing1_joint" type="fixed" >
        <origin xyz="0.6 0.0 0.82" rpy="0.0 0.0 0.0"/>
        <parent link="world" />
        <child link="railing1"/>
  </joint>
  <joint name="frame_railing2_joint" type="fixed" >
        <origin xyz="0.9 0.0 0.82" rpy="0.0 0.0 0.0"/>
        <parent link="world" />
        <child link="railing2"/>
  </joint>

<link name="railing1">
    <collision>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <geometry>
        <box size="0.01 1.5 0.01"/>-
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <geometry>
        <box size="0.01 1.5 0.01"/>-
      </geometry>
    </visual>

    <inertial>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia
        ixx="1" ixy="0.0" ixz="0.0"
        iyy="1" iyz="0.0"
        izz="1"/>
    </inertial>
  </link>

  <gazebo reference="railing1">
      <mu1>0.0000001</mu1>
      <mu2>0.0000001</mu2>
  </gazebo>

  <link name="railing2">
    <collision>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <geometry>
        <box size="0.01 1.5 0.01"/>-
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <geometry>
        <box size="0.01 1.5 0.01"/>-
      </geometry>
    </visual>

    <inertial>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia
        ixx="1" ixy="0.0" ixz="0.0"
        iyy="1" iyz="0.0"
        izz="1"/>
    </inertial>
  </link>

  <gazebo reference="railing2">
      <mu1>0.0000001</mu1>
      <mu2>0.0000001</mu2>
  </gazebo>

</xacro:if>


