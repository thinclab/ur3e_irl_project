          -
        </geometry>
      </visual>

      <inertial>
        <origin xyz="0 0 0.0" rpy="0 0 0" />
        <mass value="1" />
        <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1" />
      </inertial>
    </link>

    <gazebo reference="railing2">
      <mu1>0.0000001</mu1>
      <mu2>0.0000001</mu2>
    </gazebo>

  </xacro:if>


